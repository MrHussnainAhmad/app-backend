{
  "version": 2,
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "crons": [
    {
      "path": "/p/general/exchange-rates/cron-job",
      "schedule": "0 0 * * *"
    }
  ]
}